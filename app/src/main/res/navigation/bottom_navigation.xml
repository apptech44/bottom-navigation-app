<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/bottom_navigation"
    app:startDestination="@id/homeFragment"
    tools:ignore="UnusedNavigation">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.mybottomnivagationapp.fragments.HomeFragment"
        android:label="home_fragment"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/listFragment"
        android:name="com.example.mybottomnivagationapp.fragments.ListFragment"
        android:label="list_fragment"
        />

    <fragment
        android:id="@+id/helpFragment"
        android:name="com.example.mybottomnivagationapp.fragments.HelpFragment"
        android:label="help_fragment"
        tools:layout="@layout/fragment_help" />


</navigation>